<%- include('Header') -%>
<div class="container">
<div class="row">
    <h3 class="text-center">Display Data</h3>
    <div class="col-md-12">
        <table class="table">
            <thead>
              <tr>
                <th scope="col">S.NO.</th>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
                <th scope="col">Password</th>
                <th scope="col">Date</th>
                <th scope="col">Avatar</th>
                <th scope="col">Action</th>
              </tr>
            </thead>
            <tbody>
              <% data1.forEach((item,i=0)=>{%>
                <tr>
                    <th><%=i++ %></th>
                    <td><%= item.Name %></td>
                    <td><%= item.Email %></td>
                    <td><%= item.Password %></td>
                    <td><%= item.Date %></td>
                    <td><img src="<%= item.image.url %>" alt="" width="60" height="50"></td>
                    <td>
                      <a href="/editdata/<%= item._id %>" class="btn btn-success">Edit</a>
                      <a href="/viewdata/<%= item._id%>" class="btn btn-warning">View</a>
                      <a href="/delete/<%=item._id%>" class="btn btn-danger">Delete</a>
                    </td>
                  </tr>
             <% })%>
              
            </tbody>
          </table>
</div>
</div>

</div>






<%- include('Footer') -%>


